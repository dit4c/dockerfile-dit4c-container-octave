<?xml version="1.0" encoding="UTF-8"?>

<openbox_menu xmlns="http://openbox.org/3.4/menu">

<menu id="terminals-menu" label="Terminals" execute="/usr/libexec/openbox-xdg-menu /etc/xdg/openbox/terminals"/>

<menu id="root-menu" label="Openbox 3">
  <separator label="Apps"/>
  <item label="Octave (console)">
    <action name="Execute">
      <command>xfce4-terminal --execute "octave"</command>
    </action>
  </item>
  <item label="Octave (GUI)">
    <action name="Execute">
      <command>octave --force-gui</command>
    </action>
  </item>
  <menu id="terminals-menu"/>
  <separator/>
  <item label="Reconfigure">
    <action name="Reconfigure" />
  </item>
  <item label="Exit">
    <action name="Exit">
      <prompt>yes</prompt>
    </action>
  </item>
</menu>

</openbox_menu>
